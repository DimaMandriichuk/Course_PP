<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="155.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dimakurs.controllers.AddNewVegetableController">
    <children>
        <TextField fx:id="VegetableNameField" layoutX="14.0" layoutY="35.0" promptText="Назва овоча" />
        <Button fx:id="addNewVegetable" layoutX="255.0" layoutY="35.0" mnemonicParsing="false" onAction="#onAddNewSort" prefHeight="25.0" prefWidth="149.0" text="Додати новий овоч" />
        <Button fx:id="back" layoutX="352.0" layoutY="99.0" mnemonicParsing="false" onAction="#onBack" text="Назад" />
        <Label layoutX="14.0" layoutY="14.0" text="Введіть назву овоча" />
      <TextField fx:id="WeightVolumeField" layoutX="14.0" layoutY="112.0" promptText="Калорійність овоча" />
      <Label layoutX="14.0" layoutY="91.0" text="Введіть калорійність овоча" />
    </children>
</AnchorPane>
